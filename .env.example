# PromptDev Environment Variables
# Copy this file to .env and fill in the values

# Database password for promptdev_user
PROMPTDEV_USER_PASS=your_secure_password_here

# Test database password
PROMPTDEV_TEST_USER_PASS=your_test_password_here

# Optional: Postgres superuser password (for advanced admin tasks)
POSTGRES_SUPERPASS=

# Database defaults (usually don't need to change)
POSTGRES_USER=promptdev_user
POSTGRES_DB=promptdev_db
